<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.c22.teamC.controllers.windowControllers.OrderController">
   <children>
      <HBox fx:id="hBox" prefHeight="200.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <FlowPane fx:id="flowPane" prefHeight="200.0" prefWidth="531.0" />
            <VBox fx:id="buttonVBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <JFXButton fx:id="editButton" onAction="#editButtonPressed" text="Edit" />
                  <JFXButton fx:id="saveButton" onAction="#saveButtonPressed" text="Save" />
                  <JFXButton fx:id="deleteButton" onAction="#deleteButtonPressed" text="Delete" />
